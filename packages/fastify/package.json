{
  "name": "fastify",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "private": true,
  "type": "module",
  "main": "dist/server.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "dev": "tsx watch --clear-screen=false --env-file=.env src/server.ts",
